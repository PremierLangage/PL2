{
  "version": 1,
  "projects": {
    "platon": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/platon",
      "sourceRoot": "apps/platon/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/platon",
            "index": "apps/platon/src/index.html",
            "main": "apps/platon/src/main.ts",
            "polyfills": "apps/platon/src/polyfills.ts",
            "tsConfig": "apps/platon/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/platon/src/favicon.ico",
              "apps/platon/src/assets",
              { "glob": "**/*",  "input": "assets", "output": "./assets" },
              { "glob": "**/*", "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/", "output": "/assets/vendors/@ant-design/assets/" },
              { "glob": "**/*.md", "input": "libs/feature/web-component/src/lib", "output": "./assets/docs/components" },
              { "glob": "**/*.svg", "input": "libs/feature/web-component/src/lib", "output": "./assets/images/components" },
              { "glob": "**/*", "input": "./node_modules/intro.js/", "output": "./assets/vendors/intro.js/" },
              { "glob": "**/*", "input": "./node_modules/katex/dist/", "output": "./assets/vendors/katex/" },
              { "glob": "**/*", "input": "./node_modules/jsplumb/dist/", "output": "./assets/vendors/jsplumb/" },
              { "glob": "**/*", "input": "./node_modules/jsplumb/css/", "output": "./assets/vendors/jsplumb/css/" },
              { "glob": "**/*", "input": "./node_modules/jsxgraph/distrib/",  "output": "./assets/vendors/jsxgraph/" },
              { "glob": "**/*", "input": "./node_modules/mathlive/dist/", "output": "./assets/vendors/mathlive/" },
              { "glob": "**/*", "input": "./node_modules/jsoneditor/dist/", "output": "./assets/vendors/jsoneditor/" },
              { "glob": "**/*", "input": "./node_modules/@cisstech/nge/assets/", "output": "./assets/vendors/nge/" },
              { "glob": "**/*", "input": "./node_modules/monaco-editor/min", "output": "./assets/vendors/nge/monaco/min" },
              { "glob": "**/*", "input": "./node_modules/monaco-editor/min-maps", "output": "./assets/vendors/nge/monaco/min-maps" },
              { "glob": "**/*.js",  "input": "./node_modules/emoji-toolkit/lib/js/", "output": "./assets/vendors/emoji-toolkit/" },
              { "glob": "viz.js", "input": "./node_modules/viz.js/", "output": "./assets/vendors/viz/"  }
            ],
            "styles": [
              "styles/app.scss",
              "apps/platon/src/styles.scss",
              { "input": "styles/ng-zorro/dark.less", "bundleName": "styles.ng-zorro.dark", "inject": false },
              { "input": "styles/ng-zorro/light.less", "bundleName": "styles.ng-zorro.light", "inject": false },
              { "input": "styles/material/dark.scss", "bundleName": "styles.material.dark", "inject": false },
              { "input": "styles/material/light.scss", "bundleName": "styles.material.light", "inject": false }
            ],
            "scripts": [
              { "bundleName": "polyfill-webcomp-es5", "input": "node_modules/@webcomponents/webcomponentsjs/custom-elements-es5-adapter.js" },
              { "bundleName": "polyfill-webcomp", "input": "node_modules/@webcomponents/webcomponentsjs/bundles/webcomponents-sd-ce-pf.js" }
            ],
            "allowedCommonJsDependencies": ["hammerjs", "d3-graphviz"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/platon/src/environments/environment.ts",
                  "with": "apps/platon/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "platon:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "platon:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "platon:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/platon/src/test.ts",
            "tsConfig": "apps/platon/tsconfig.spec.json",
            "karmaConfig": "apps/platon/karma.conf.js",
            "polyfills": "apps/platon/src/polyfills.ts",
            "styles": [],
            "scripts": [],
            "assets": []
          }
        }
      },
      "tags": ["scope:platon"]
    },
    "platon-e2e": {
      "root": "apps/platon-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/platon-e2e/protractor.conf.js",
            "devServerTarget": "platon:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "platon:serve:production"
            }
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["platon"]
    },
