    "jsoneditor": "^9.1.1",
    "jsplumb": "^2.14.6",
    "jsxgraph": "^1.1.0",
    "katex": "^0.12.0",
    "marked": "^2.1.3",
    "mathlive": "^0.59.0",
    "monaco-editor": "^0.25.2",
    "ng-event-bus": "^2.0.0",
    "ng-zorro-antd": "^11.2.0",
